<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF"
    android:orientation="vertical" >

<LinearLayout
    android:id="@+id/list_item_layout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_weight="0"
    android:background="#ececec"
    android:clickable="false"
    android:longClickable="false" >

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_margin="3dp"
        android:layout_weight="25"
        android:background="@anim/shape" >

        <WebView
            android:id="@+id/webView1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_margin="2dp"
            android:layout_weight="25"
            android:clickable="false"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:longClickable="false" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="2dp"
        android:paddingLeft="5dp"
        android:layout_weight="55"
        android:clickable="false"
        android:focusable="false"
        android:longClickable="false"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView_01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#ececec"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textMultiLine"
                android:longClickable="false"
                android:text="이름/"
                android:textColor="#000000"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView_02"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginTop="5dp"
                android:background="#ececec"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textMultiLine"
                android:longClickable="false"
                android:text="￦6,000"
                android:textColor="#858585"
                android:textSize="15dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="15dp"
            android:layout_marginTop="2dp" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:src="@drawable/prolist_star100" />
            
            <ImageView
                android:id="@+id/AnswerPlayButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:src="@drawable/prolist_star100" />
            
            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:src="@drawable/prolist_star100" />
            
            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:src="@drawable/prolist_star100" />
            
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:src="@drawable/prolist_star100" />
            
            <TextView
                android:id="@+id/TextView_00"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:background="#ececec"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:longClickable="false"
                android:paddingLeft="5dp"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="100명 5.0점"
                android:textColor="#858585"
                android:textSize="10dp" />

        </LinearLayout>
        
        <TextView
            android:id="@+id/TextView_03"
            android:layout_width="fill_parent"
            android:layout_height="15dp"
            android:layout_marginTop="3dp"
            android:background="#ececec"
            android:clickable="false"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:inputType="textMultiLine"
            android:longClickable="false"
            android:text="certification"
            android:textColor="#858585"
            android:textSize="12dp" />
        
        <TextView
            android:id="@+id/TextView_04"
            android:layout_width="fill_parent"
            android:layout_height="15dp"
            android:background="#ececec"
            android:clickable="false"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:inputType="textMultiLine"
            android:longClickable="false"
            android:text="profile"
            android:textColor="#858585"
            android:textSize="12dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/likestar"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="15" >

        <ImageView
            android:id="@+id/ImageView_9"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:background="@drawable/prolist_fav01"
            android:scaleType="fitXY" />

    </LinearLayout>

</LinearLayout>

</LinearLayout>